<launch>
	<!-- Launch the camera -->
	<include file="$(find urVision)/launch/rpi_cam.launch"></include>

	<!-- Launch urVision node -->
	<node pkg="urVision" type="urVision" name="urVision" output="screen">
		<rosparam command="load" file="$(find urVision)/config/default.yaml" />
	</node>
</launch>
